# Choose your embedding provider: 'openai' or 'huggingface'
VITE_EMBEDDING_PROVIDER=huggingface

# OpenAI Configuration (if using OpenAI embeddings)
VITE_OPENAI_API_KEY=your_openai_api_key_here

# Hugging Face Configuration (if using HF embeddings) 
# Get free API key from: https://huggingface.co/settings/tokens
VITE_HUGGINGFACE_API_KEY=your_huggingface_api_key_here

# Backend API Configuration
VITE_API_BASE_URL=http://localhost:8080

# Google Cloud Configuration (for pgvector)
VITE_GOOGLE_CLOUD_PROJECT_ID=your_project_id
VITE_DB_HOST=your_db_host
VITE_DB_NAME=your_database_name
VITE_DB_USER=your_db_user
VITE_DB_PASSWORD=your_db_password

# Optional: Vector Database Configuration
VITE_USE_VECTOR_DATABASE=false
VITE_VECTOR_DB_TYPE=pgvector  # Options: pgvector, pinecone, weaviate, chroma

# Legacy support (also works)
# REACT_APP_EMBEDDING_PROVIDER=huggingface
# REACT_APP_OPENAI_API_KEY=your_openai_api_key_here
# REACT_APP_HUGGINGFACE_API_KEY=your_huggingface_api_key_here